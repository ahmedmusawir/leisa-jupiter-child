/*================================================
=            Work With Me Page Styles            =
================================================*/

#work-with-me-block-1 {
	background: url('/wp-content/uploads/2016/06/block-4-bg.jpg') repeat center center fixed;
	padding-top: 5rem;
	padding-bottom: 5rem;
	padding-left: 2rem;
	padding-right: 2rem;

	h1.headline {
		font-style: italic;
		font-size: 3.5rem;
		color: lighten( gray, 20% );

		i {
			font-size: 7rem;
		}
	}

	h2 {
		padding-top: 5rem;
		padding-bottom: 3rem;
		font-size: 3.5rem;
		color: gray;

			position: relative;


			&:before {

			  content : "";

			  position: absolute;

			  left    : 30%;

			  bottom  : 0;

			  height  : 1px;

			  width   : 40%;  /* or 100px */

				border-bottom: .4rem solid $leisa-green;

			}		
	}

	@media ( min-width: 640px ) {
		padding-left: 5rem;
		padding-right: 5rem;
	}
	@media ( min-width: 768px ) {
		padding-left: 8rem;
		padding-right: 8rem;
		padding-top: 5rem;
		padding-bottom: 5rem;
	}		
	@media ( min-width: 1200px ) {
		padding-left: 10rem;
		padding-right: 10rem;
		padding-top: 8rem;
		padding-bottom: 8rem;

		h1.headline {
			font-style: italic;
			font-size: 5.5rem;
			color: lighten( gray, 20% );

			i {
				font-size: 10rem;
			}
		}		
	}	

	#accordion {

		padding-top: 3.5rem;

		a {
			color: lighten( gray, 20% );

			&:hover, &:active, &:focus {
				text-decoration: none;
				color: lighten( orange, 15% );
			}
		}

		.panel-heading {
			border: 1rem solid lighten( gray, 30% );
			background-image: url('/wp-content/uploads/2016/07/panel-header-bg.jpg');

			h4 {
				font-size: 3rem;
				font-weight: 600;	
				font-variant: small-caps;			
			}
		}

		.panel-body {
			.txt-block {
				margin-top: 5rem;
				font-size: 2rem;
				margin-bottom: 3rem;

				@media ( min-width: 1200px ) {
					font-size: 2.5rem;
				}
				@media ( min-width: 1500px ) {
					font-size: 3rem;
					padding-left: 5rem;
					// padding-top: 7rem;
				}				
			}
		}
	}	
}


#work-with-me-block-2 {

	.content-wrapper {
		background: url('/wp-content/uploads/2016/07/block1-bg-4.jpg') center center fixed;
		text-align: center;
		padding-top: 10rem;
		padding-bottom: 10rem;  

		h2 {
			padding-bottom: 5rem;
		}

		#carousel-example-captions {

			.carousel-indicators {
				margin-top: 15rem;
				top: 50%;
			}
			.carousel-control.left {
				background-image: none !important;
			}
			.carousel-control.right {
				background-image: none !important;
			}

			.item {
				img {
					width: 10%;
				}
				i {
					font-size: 4rem;
					color: lighten( $gray-light, 10% );
				}				
			}
			
		}
	}

}

/*=====  End of Work With Me Page Styles  ======*/



/*=====================================
=            Footer Styles            =
=====================================*/

#mk-footer {

	// border: 1rem solid red;
	min-height: 500px;
	text-align: center;

	background: url("/wp-content/uploads/2016/06/footer-bg-1-1.jpg") no-repeat center center;

	  -webkit-background-size: cover;

	  -moz-background-size: cover;

	  -o-background-size: cover;

	  background-size: cover;	

	  /*----------  Footer Logo  ----------*/
	  #footer-logo {
	  	padding-top: 5rem;
	  }

	  /*----------  Footer Menu  ----------*/
	  #menu-main-2 {

	  	padding-top: 3rem;
	  	padding-bottom: 3rem;

	  	li {
	  		display: inline-block;

	  		a {
	  			font-family: 'Raleway', sans-serif !important;
	  			font-weight: 700;
	  			font-size: 2.8rem;
	  			text-transform: uppercase;
	  			padding-right: 2rem;
	  			color: lighten( $leisa-green, 10% );

	  			&:hover {
	  				text-decoration: none;
	  				color: orange;
	  			}
	  		}
	  	}
	  }

	  /*----------  Footer Social  ----------*/
	  .footer-social {
	  	li {
	  		display: inline-block;
	  		height: 80px;
	  		width: 80px;
	  		border-radius: 90%;
	  		background-color: black;

	  		i {
	  			color: lighten( #61973c, 15% );
	  			padding-right: 2rem;	
	  			padding-top: 2rem;
	  			padding-left: 1.5rem;

	  			&:hover {
	  				color: darken( orange, 0% );
	  			}
	  		}
	  	}
	  }

	  /*----------  Copyright  ----------*/
	  #copyright {
	  	color: $leisa-green;
	  }
}

/*=====  End of Footer Styles  ======*/




























