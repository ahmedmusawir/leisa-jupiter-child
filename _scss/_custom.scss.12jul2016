html { 
 
	// display: none;

}

body {

	// font-family: 'Muli', sans-serif;
	font-family: 'Raleway', sans-serif !important;

	h1,h2,h3,h4,h5,h6,p {
		font-family: 'Raleway', sans-serif !important;

	}
	a {
		text-decoration: none !important;
	}
	// display: none;
}

#mk-boxed-layout {

	// border: 1rem solid blue;
	z-index: 100;

	.mk-header-nav-container {
		// border: 1rem solid red;
	}
	.mk-header-holder {
		// display: none;
		.mk-header-toolbar {
			background-color: transparent;

			@media ( max-width: 1139px ) {
				display: none;

			}
		}
	}	
}

#header {
	background-color: black;
	position: relative;
	z-index: 1;
	overflow: hidden;

	// background: url("/wp-content/uploads/2016/04/Cover-Photo.jpg") no-repeat center center fixed;

	//   -webkit-background-size: cover;

	//   -moz-background-size: cover;

	//   -o-background-size: cover;

	//   background-size: cover;	



	@media ( max-width: 1400px ) {
		img.large {
			display: none;
		}
		img.medium {
			display: block;
		}		
	}
	@media ( min-width: 1401px ) {
		img.large {
			display: block;
		}
		img.medium {
			display: none;
		}		
	}

	.header-txt-block {
		position: absolute;
		top: 20%;
		left: 10%;
		padding: 15rem;

		background-color: rgba( black, .5 );

		@media ( min-width: 1401px ) {

			top: 5%;
			padding: 10rem;
		}					

			
		@media ( max-width: 1400px ) {

			top: 20%;
			padding: 10rem;
			// border: 1rem solid red;
		}	
			
		@media ( max-width: 1200px ) {

			top: 15%;
			padding: 10rem;
		}	
		
		@media ( max-width: 800px ) {
			
			top: 6%;
			left: 6%;
			padding: 10rem;	
			// border: 1rem solid red;	
		}		

		@media ( max-width: 700px ) {
			
			top: 1%;
			left: 2%;
			padding: 8rem;	
			padding-left: 2rem;
			padding-right: 2rem;
			padding-top: 2rem;
			// border: 1rem solid red;	
		}

	

		.txt-content {
			
			h1 {
				font-size: 5rem;
				font-weight: 700;
				color: $roy-green;
			}
			h3 {
				font-size: 3.5rem;
				font-weight: 600;
				color: lighten( orange, 15% );
			}
			@media ( max-width: 850px ) {
				h1 {
					font-size: 3rem;
				}
				h3 {
					font-size: 2rem;
				}
			}							

			@media ( max-width: 500px ) {
				
				h1 {
					font-size: 3rem;
				}
				h3 {
					font-size: 2rem;
				}
			}
			@media ( max-width: 400px ) {
				
				h1 {
					font-size: 2rem;
				}
				h3 {
					font-size: 1.5rem;
				}
			}			
			@media ( min-width: 1750px ) {

				h1 {
					font-size: 8rem;
				}
				h3 {
					font-size: 5.5rem;
				}
				
			}			
		}
	}
}

#mk-theme-container {

	.mk-header {
    	border-bottom: 0px solid #ededed;
	}
}

