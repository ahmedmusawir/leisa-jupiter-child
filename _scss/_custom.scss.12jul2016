=            Home page style            =
=======================================*/

.page-id-12300 {

	.mk-header-nav-container {
		display: none;
	}
	.header-logo {
		display: none;
	}
	.mk-header-holder {
		display: none;
	}
}

#block-1 {
	background-color: black;
	padding-top: 2rem;
	padding-bottom: 2rem;
}

#block-2 {
	// min-height: 1000px;
	// border: 1rem solid red;
	position: relative;
	overflow: hidden;
	z-index: -100;

	background: url("/wp-content/uploads/2016/06/video-alternative-bg.jpg") no-repeat center center;

	  -webkit-background-size: cover;

	  -moz-background-size: cover;

	  -o-background-size: cover;

	  background-size: cover;	

	@media ( max-width: 800px ) {
		background-image: url('/wp-content/uploads/2016/06/video-alternative-bg.jpg');
	}  

	// background-color: transparentize( lighten( $gray, 10% ), .3 );

	.video-container {


		video#second-section-video {
			position: absolute;
			top: 0;
			right: 0;
			left: 0;
			min-width: 100%;
			min-height: 100%;
			width: auto;
			height: auto;
			z-index: -99;
			// background: url("img/second-section.jpg") no-repeat;
			background-size: cover;
		}

		.text-content {
			
			background-color: transparentize( lighten( $leisa-green, 10% ), .3 );

			padding-top: 2rem;
			padding-bottom: 2rem;
			margin-bottom: 2rem;
			margin-left: 2rem;
			margin-right: 2rem;

			h3 {
				padding-bottom: 2rem;
				color: white;
				font-size: 5rem;
				font-family: 'Raleway', sans-serif !important;
			}
			p {
				color: white;
				font-size: 3.5rem;
				font-family: 'Raleway', sans-serif !important;
			}

			@media ( max-width: 400px ) {
				h3 {
					font-size: 3rem;
				}
				p {
					font-size: 2rem;
				}
			}
		}

		.download-box-container {

			background-color: transparentize( lighten( white, 10% ), .2 );
			padding-top: 5rem;
			padding-bottom: 5rem;
			text-align: center;
			color: black;


			.download-box-1 {
				@include transition(all 1s ease-in-out);
				
				&:hover {
					margin-top: -2rem;
				}
			}

			figure {
				padding-bottom: 3rem;
				min-height: 250px;

			}
			h4 {
				font-size: 2.5rem;
				font-weight: 400;
				color: black;
				font-family: 'Raleway', sans-serif !important;
				min-height: 130px;
			}

			@media ( min-width: 768px ) and ( max-width: 991px ) {

				figure {
					padding-bottom: 3rem;
					min-height: 50px;

				}
				h4 {
					font-size: 2rem;
					font-weight: 400;
					color: black;
					font-family: 'Raleway', sans-serif !important;
					min-height: 150px;
				}				
			}
		}

	}
}

